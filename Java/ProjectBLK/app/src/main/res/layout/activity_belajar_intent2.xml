<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BelajarIntent.BelajarIntent2">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="HALAMAN KEDUA"
        android:textAllCaps="true"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="222dp"
        android:layout_marginBottom="181dp"
        android:layout_above="@+id/btn_ketiga_2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/btn_ketiga_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/btn_kesatu_2"
        android:layout_centerHorizontal="true"
        android:text="KE HALAMAN KE TIGA"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="259dp" />

    <Button
        android:id="@+id/btn_kesatu_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn_ketiga_2"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/btn_ketiga_2"
        android:text="KE HALAMAN UTAMA" />

</RelativeLayout>